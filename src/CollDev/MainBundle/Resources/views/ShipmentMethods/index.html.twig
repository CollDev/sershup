{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ShipmentMethods list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Virtuemartvendorid</th>
                <th>Shipmentjpluginid</th>
                <th>Slug</th>
                <th>Shipmentelement</th>
                <th>Shipmentparams</th>
                <th>Ordering</th>
                <th>Shared</th>
                <th>Published</th>
                <th>Createdon</th>
                <th>Createdby</th>
                <th>Modifiedon</th>
                <th>Modifiedby</th>
                <th>Lockedon</th>
                <th>Lockedby</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('shipmentmethods_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.virtuemartVendorId }}</td>
                <td>{{ entity.shipmentJpluginId }}</td>
                <td>{{ entity.slug }}</td>
                <td>{{ entity.shipmentElement }}</td>
                <td>{{ entity.shipmentParams }}</td>
                <td>{{ entity.ordering }}</td>
                <td>{{ entity.shared }}</td>
                <td>{{ entity.published }}</td>
                <td>{% if entity.createdOn %}{{ entity.createdOn|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.createdBy }}</td>
                <td>{% if entity.modifiedOn %}{{ entity.modifiedOn|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.modifiedBy }}</td>
                <td>{% if entity.lockedOn %}{{ entity.lockedOn|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.lockedBy }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('shipmentmethods_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('shipmentmethods_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('shipmentmethods_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
